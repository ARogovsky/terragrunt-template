module "setup_key" {
  source  = "cloudposse/kms-key/aws"
  version = "0.4.0"

  namespace               = "{{ uniq_id }}"
  stage                   = "setup"
  name                    = "{{ project_name }}"
  description             = "KMS key to secure state"
  deletion_window_in_days = 7
  alias                   = "alias/{{ project_name }}"
}
